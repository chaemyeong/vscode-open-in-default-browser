{
    "name": "open-html-in-browser-ml",
    "displayName": "Open HTML in Default Browser ML",
    "description": "Open HTML in Default Browser ML",
    "version": "0.0.5",
    "publisher": "cmlim107",
    "repository": {
        "type": "git",
        "url": "https://github.com/cmlim107/vscode-open-in-default-browser.git"
    },
    "bugs": {
        "url": "https://github.com/cmlim107/vscode-open-in-default-browser/issues"
    },
    "engines": {
        "vscode": "^1.10.0"
    },
    "categories": [
        "Other"
    ],
    "keywords": [
        "open",
        "html",
        "default",
        "browser"
    ],
    "icon": "icon.jpg",
    "activationEvents": [
        "onCommand:cmlim107.openInBrowser"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [
            {
                "command": "cmlim107.openInBrowser",
                "title": "%extension.openhtmlinbrowser.title%"
            }
        ],
        "menus": {
            "editor/context": [
                {
                    "when": "editorLangId == html",
                    "command": "cmlim107.openInBrowser",
                    "group": "1_modification"
                }
            ],
            "explorer/context": [
                {
                    "when": "resourceLangId == html",
                    "command": "cmlim107.openInBrowser",
                    "group": "1_modification"
                }
            ]
        }
    },
    "scripts": {
        "pub": "vsce publish patch"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.32",
        "@types/node": "^6.0.40",
        "eslint": "^3.6.0",
        "mocha": "^2.3.3",
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "vscode-nls-dev": "^3.2.1"
    },
    "dependencies": {
        "opn": "^4.0.2",
        "vscode-nls": "^3.2.4"
    }
}
